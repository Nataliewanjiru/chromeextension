{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Add the reading time to Chrome Extension documentation articles",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
       "default_width": 300,
     "default_height": 600 
    },
    "permissions":[
        "scripting"
    ],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ]
}